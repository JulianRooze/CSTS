<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Copyright />
    <PackageId>CSTS</PackageId>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.1.5</Version>
    <Authors>Julian Rooze</Authors>
    <Company />
    <Product />
    <Description>A simple library that can convert .NET types to TypeScript types. The output can be both a .ts file with classes as well as a .d.ts file with only the interface definitions.</Description>
    <PackageProjectUrl>https://github.com/JulianRooze/CSTS/blob/master/README.md</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/JulianRooze/CSTS/master/license.txt</PackageLicenseUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.4.0" />
  </ItemGroup>
</Project>